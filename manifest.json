{
	"content_scripts": [ {
		"js": ["js/jquery-1.8.3.min.js", "js/vchrome.js"],
		"matches": [ "http://vkontakte.ru/*", "http://vk.com/*" ],
		"run_at": "document_start"
	}],
	"icons": {
		"16": "img/vchrome_16.png",
		"48": "img/vchrome_48.png",
		"128": "img/vchrome_128.png"
	},
	"web_accessible_resources": [
		"img/disk-blue.png",
		"img/disk-white.png",
		"js/jquery-1.8.3.min.js",
		"js/vchrome.js"
	],
	"description": "VKontakte Chrome Extension",
	"name": "VChrome",
	"permissions": [ "tabs", "http://vkontakte.ru/*", "http://vk.com/*" ],
	"version": "0.6.1",
	"manifest_version": 2
}
